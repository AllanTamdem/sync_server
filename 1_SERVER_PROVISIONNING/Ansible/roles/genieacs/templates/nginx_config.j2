server {

    listen 443;
    server_name {{domainTr069Mediapost}};

    ssl on;
    ssl_certificate {{certificationDir}}/server.crt;
    ssl_certificate_key {{certificationDir}}/server.key;

    access_log /var/log/nginx/genieacs-tr069.log combined;
    error_log /var/log/nginx/genieacs-tr069.log;

    location / {
        auth_basic "ms";
        auth_basic_user_file /etc/nginx/ms-htpasswd;
        proxy_set_header Authorization "";
        proxy_pass http://127.0.0.1:7547;
    }
}
