
				<h5>last 20 batch logs :</h5>

				<table class="table table-hover table-condensed table-bordered table-striped">
					<thead>
						<tr>
							<th>Date</th>
							<th>Result</th>
						</tr>
					</thead>
				<% @logs.each{ |log| %>
					<tr>
						<td><%= log[:created_at_ago] %> (<%= log[:created_at] %>)</td>
						<td><pre><%= log[:content] %></pre></td>
					</tr>
				<%}%>				
				</table>